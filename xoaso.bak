const fi = 'xoaso.inp';
	  fo = 'xoaso.out';
var a,s:ansistring;
	amax,amax2:char;
	k,n:longint;

procedure input;
var f:text;
begin
	assign(f,fi);
	reset(f);
	readln(f,a);
	read(f,k);
	close(f);
end;

procedure main;
var i,j:longint;
	tmp:char;
begin
	amax:=a[1];
	amax2:='0';
	n:=length(a);
	for i:=2 to k+1 do
		if a[i] > amax then
		begin
			amax2:=amax;
			amax:=a[i];
		end;
	s:=s+amax;
	amax:='0';
	for j:=2 to n-k do
	//for i:=j to k+j do
	begin
		if a[k+j] >= amax then
		begin
			amax:=a[i];
			if amax > amax2 then
			begin
				tmp:=amax;
				amax:=amax2;
				amax2:=tmp;
			end;
		end;
		s:=s+amax;
		amax:='0';
	end;
end;

procedure output;
var f:text;
begin
	assign(f,fo);
	rewrite(f);
	write(f,s);
	close(f);
end;

begin
	input;
	main;
	output;
end.
